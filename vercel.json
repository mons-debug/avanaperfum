{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "s-maxage=1, stale-while-revalidate=59"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "regions": ["cdg1"],
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 10
    }
  },
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}
